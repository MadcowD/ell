{
    "configurations": [
        {
            "name": "studio:server:dev",
            "type": "python",
            "request": "launch",
            "module": "ell2a.studio",
            "args": [
                "--storage-dir",
                "${workspaceFolder}/./logdir",
                "--dev"
            ],
            "justMyCode": false
        },
        {
            "type": "node",
            "request": "launch",
            "name": "studio:client:dev",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "start:dev"
            ],
            "cwd": "${workspaceFolder}/ell2a-studio",
            "console": "integratedTerminal"
        }
    ]
}