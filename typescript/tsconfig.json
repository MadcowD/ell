{
  "compilerOptions": {
    "target": "es2022",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "lib": ["es2023", "DOM"],
    "declaration": true,
    "declarationMap": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "exactOptionalPropertyTypes": false,
    "skipLibCheck": true,
    "outDir": "./dist",
    "noErrorTruncation": false,
    "paths": {
      "ell-ai": ["./src/index.ts"],
      "ell-ai/*": ["./src/*"]
    }
  }
}
